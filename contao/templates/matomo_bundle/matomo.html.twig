{% extends "@Contao/be_main" %}
{% trans_default_domain 'AnalyticsBundle' %}
{% block main_content %}
  <div class="space-y-8 bg-[#eaeaec] -m-px dark:bg-[#121416]">

    <div class="space-y-8">
      <div class="px-4 sm:flex sm:items-center">
        <div class="sm:flex-auto">
          <h2 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "insight.title" | trans }}</h2>
        </div>
        <div class="mt-4 sm:mt-0 sm:ml-16 sm:flex-none">
          <a href="{{ objContent.analytics_remote_url }}?module=CoreHome&action=index&idSite={{ objContent.analytics_remote_id }}&period=month&date=today" target="_blank" class="block rounded-md bg-sky-500 dark:bg-sky-700 transition-all px-3 py-2 text-center text-sm font-semibold text-white! hover:bg-sky-400 dark:hover:bg-sky-900 focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-sky-500">{{ "insight.analytics_remote_url" | trans }}</a>
        </div>
      </div>
      <dl class="grid grid-cols-1 gap-5 sm:grid-cols-2 md:grid-cols-4">
        <div class="overflow-hidden rounded-lg bg-white dark:bg-[#1b1d21] px-4 py-5 shadow-sm sm:p-6">
          <dt class="truncate text-sm font-medium text-neutral-500 dark:text-neutral-300">{{ "insight.nb_uniq_visitors" | trans }}</dt>
          <dd class="mt-1 text-3xl font-semibold tracking-tight text-neutral-900 dark:text-neutral-300">{{ insight.nb_uniq_visitors }}</dd>
        </div>
        <div class="overflow-hidden rounded-lg bg-white dark:bg-[#1b1d21] px-4 py-5 shadow-sm sm:p-6">
          <dt class="truncate text-sm font-medium text-neutral-500 dark:text-neutral-300">{{ "insight.nb_actions" | trans }}</dt>
          <dd class="mt-1 text-3xl font-semibold tracking-tight text-neutral-900 dark:text-neutral-300">{{ insight.nb_actions }}</dd>
        </div>
        <div class="overflow-hidden rounded-lg bg-white dark:bg-[#1b1d21] px-4 py-5 shadow-sm sm:p-6">
          <dt class="truncate text-sm font-medium text-neutral-500 dark:text-neutral-300">{{ "insight.bounce_rate" | trans }}</dt>
          <dd class="mt-1 text-3xl font-semibold tracking-tight text-neutral-900 dark:text-neutral-300">{{ insight.bounce_rate }}</dd>
        </div>
        <div class="overflow-hidden rounded-lg bg-white dark:bg-[#1b1d21] px-4 py-5 shadow-sm sm:p-6">
          <dt class="truncate text-sm font-medium text-neutral-500 dark:text-neutral-300">{{ "insight.nb_pageviews" | trans }}</dt>
          <dd class="mt-1 text-3xl font-semibold tracking-tight text-neutral-900 dark:text-neutral-300">{{ insight.nb_pageviews }}</dd>
        </div>
      </dl>
    </div>

    <div class="grid grid-cols-1 gap-5 sm:grid-cols-2">

      <div class="px-4 space-y-8">
        <div class="">
          <div class="sm:flex sm:items-center">
            <div class="sm:flex-auto">
              <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "browser.title" | trans }}</h3>
              <p class="mt-2 text-sm text-neutral-700 dark:text-neutral-300">{{ "browser.txt" | trans }}</p>
            </div>
          </div>
          <div class="mt-1 flow-root">
            <div class="-mx-4 -my-2 overflow-x-auto ">
              <div class="inline-block min-w-full py-2 align-middle">
                <div class="overflow-hidden shadow-sm ring-1 ring-black/5 sm:rounded-lg">
                  <table class="min-w-full divide-y divide-neutral-200 dark:divide-neutral-700">
                    <thead class="bg-neutral-50 dark:bg-[#1b1d21]">
                    <tr>
                      <th scope="col" class="py-3.5 pr-3 pl-4 text-left text-sm font-semibold text-neutral-900 dark:text-white sm:pl-6">{{ "browser.label" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "browser.nb_visits" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "browser.nb_actions" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "browser.bounce_count" | trans }}</th>
                      <th scope="col" class="pr-3.5 pr-3 pl-4 text-left text-sm font-semibold text-neutral-900 dark:text-white sm:pr-6">{{ "browser.nb_visits_converted" | trans }}</th>
                    </tr>
                    </thead>
                    <tbody class="divide-y divide-neutral-200 dark:divide-neutral-700 bg-white dark:bg-[#121416]">
                    {% for browser in browsers %}
                      <tr class="even:bg-neutral-50 even:dark:bg-[#1b1d21]">
                        <td class="py-2 pr-3 pl-4 text-sm font-medium whitespace-nowrap text-neutral-900 dark:text-neutral-200 sm:pl-3">{{ browser.label ?? "N.C." }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ browser.nb_visits ?? "0" }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ browser.nb_actions ?? "0" }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ browser.bounce_count ?? "0" }}</td>
                        <td class="py-2 pr-4 pl-3 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ browser.nb_visits_converted ?? "0" }}</td>
                      </tr>
                    {% endfor %}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div>
          <div class="sm:flex sm:items-center">
            <div class="sm:flex-auto">
              <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "user_language.title" | trans }}</h3>
              <p class="mt-2 text-sm text-neutral-700 dark:text-neutral-300">{{ "user_language.txt" | trans }}</p>
            </div>
          </div>
          <div class="mt-1 flow-root">
            <div class="-mx-4 -my-2 overflow-x-auto ">
              <div class="inline-block min-w-full py-2 align-middle">
                <div class="overflow-hidden shadow-sm ring-1 ring-black/5 sm:rounded-lg">
                  <table class="min-w-full divide-y divide-neutral-200 dark:divide-neutral-700">
                    <thead class="bg-neutral-50 dark:bg-[#1b1d21]">
                    <tr>
                      <th scope="col" class="py-3.5 pr-3 pl-4 text-left text-sm font-semibold text-neutral-900 dark:text-white sm:pl-6">{{ "user_language.label" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "user_language.nb_uniq_visitors" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "user_language.nb_actions" | trans }}</th>
                      <th scope="col" class="px-3 py-3.5 text-left text-sm font-semibold text-neutral-900 dark:text-white">{{ "user_language.bounce_count" | trans }}</th>
                      <th scope="col" class="pr-3.5 pr-3 pl-4 text-left text-sm font-semibold text-neutral-900 dark:text-white sm:pr-6">{{ "user_language.nb_visits_converted" | trans }}</th>
                    </tr>
                    </thead>
                    <tbody class="divide-y divide-neutral-200 dark:divide-neutral-700 bg-white dark:bg-[#121416]">
                    {% for language in user_language %}
                      <tr class="even:bg-neutral-50 even:dark:bg-[#1b1d21]">
                        <td class="py-2 pr-3 pl-4 text-sm font-medium whitespace-nowrap text-neutral-900 dark:text-neutral-200 sm:pl-3">{{ language.label ?? "N.C." }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ language.nb_uniq_visitors ?? "0" }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ language.nb_actions ?? "0" }}</td>
                        <td class="px-3 py-2 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ language.bounce_count ?? "0" }}</td>
                        <td class="py-2 pr-4 pl-3 text-sm whitespace-nowrap text-neutral-500 dark:text-neutral-200">{{ language.nb_visits_converted ?? "0" }}</td>
                      </tr>
                    {% endfor %}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>


      <div class="space-y-8">
        <div class="">
          <div class="px-4 sm:px-6 lg:px-8">
            <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "insight.conversion.title" | trans }}</h3>
            <p class="mt-1 max-w-2xl text-sm/6 text-neutral-500 dark:text-neutral-200">{{ "insight.conversion.txt" | trans }}</p>
          </div>
          <div class="mt-3 overflow-hidden bg-white dark:bg-[#1b1d21] shadow-sm sm:rounded-lg">
            <dl class="divide-y divide-neutral-200 dark:divide-neutral-700">
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.conversion.conversion_rate" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.conversion_rate }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.conversion.conversion_rate_new_visit" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.conversion_rate_new_visit }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.conversion.conversion_rate_returning_visit" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.conversion_rate_returning_visit }}</dd>
              </div>
            </dl>
          </div>
        </div>

        <div class="">
          <div class="px-4 sm:px-6 lg:px-8">
            <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "insight.referrers.title" | trans }}</h3>
            <p class="mt-1 max-w-2xl text-sm/6 text-neutral-500 dark:text-neutral-200">{{ "insight.referrers.txt" | trans }}</p>
          </div>
          <div class="mt-3 overflow-hidden bg-white dark:bg-[#1b1d21] shadow-sm sm:rounded-lg">
            <dl class="divide-y divide-neutral-200 dark:divide-neutral-700">
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.referrers.Referrers_visitorsFromSearchEngines" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.Referrers_visitorsFromSearchEngines }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.referrers.Referrers_visitorsFromDirectEntry" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.Referrers_visitorsFromDirectEntry }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.referrers.Referrers_visitorsFromSocialNetworks" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.Referrers_visitorsFromSocialNetworks }}</dd>
              </div>
            </dl>
          </div>
        </div>

        <div class="">
          <div class="px-4 sm:px-6 lg:px-8">
            <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "insight.returning.title" | trans }}</h3>
            <p class="mt-1 max-w-2xl text-sm/6 text-neutral-500 dark:text-neutral-200">{{ "insight.returning.txt" | trans }}</p>
          </div>
          <div class="mt-3 overflow-hidden bg-white dark:bg-[#1b1d21] shadow-sm sm:rounded-lg">
            <dl class="divide-y divide-neutral-200 dark:divide-neutral-700">
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.returning.nb_uniq_visitors_returning" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.nb_uniq_visitors_returning }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.returning.bounce_rate_returning" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.bounce_rate_returning }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.returning.nb_actions_per_visit_returning" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.nb_actions_per_visit_returning }}</dd>
              </div>
            </dl>
          </div>
        </div>

        <div class="">
          <div class="px-4 sm:px-6 lg:px-8">
            <h3 class="text-base font-semibold text-neutral-900 dark:text-white dark:text-white">{{ "insight.other.title" | trans }}</h3>
            <p class="mt-1 max-w-2xl text-sm/6 text-neutral-500 dark:text-neutral-200">{{ "insight.other.txt" | trans }}</p>
          </div>
          <div class="mt-3 overflow-hidden bg-white dark:bg-[#1b1d21] shadow-sm sm:rounded-lg">
            <dl class="divide-y divide-neutral-200 dark:divide-neutral-700">
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.nb_downloads" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.nb_downloads }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.nb_searches" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.nb_searches }}</dd>
              </div>
              <div class="px-2 py-4 sm:grid sm:grid-cols-3 sm:gap-4 sm:px-6">
                <dt class="text-sm font-medium text-neutral-900 dark:text-white">{{ "insight.avg_time_dom_completion" | trans }}</dt>
                <dd class="mt-1 text-sm/6 text-neutral-700 dark:text-neutral-300 sm:col-span-2 sm:mt-0">{{ insight.avg_time_dom_completion }}</dd>
              </div>
            </dl>
          </div>
        </div>
      </div>
    </div>
  </div>

{% endblock %}